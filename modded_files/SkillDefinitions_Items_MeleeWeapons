   <!-- Sword -->
   <SkillDefinition Id="Slice">
      <ActionPointsCost>1</ActionPointsCost>
      <UsesPerTurnCount>4</UsesPerTurnCount>
      <Range Min="1" Max="1" CardinalDirectionOnly="true"/>
      <SkillAction>
         <Attack>
            <AttackType>Physical</AttackType>
            <DamageMultiplier>1</DamageMultiplier>
            <SkillEffects>
               <Follow/>
               <Momentum>
                  <DamageBonusPerTile>0.3</DamageBonusPerTile>
               </Momentum>
            </SkillEffects>
         </Attack>
      </SkillAction>

      <CastFXs TotalDuration="0.25">
         <VisualEffect>
            <Paths>
               <Path Orientation="S">ShortSword/Slice/Slice_Caster_S</Path>
               <Path Orientation="N">ShortSword/Slice/Slice_Caster_N</Path>
               <Path Orientation="W">ShortSword/Slice/Slice_Caster_W</Path>
               <Path Orientation="E">ShortSword/Slice/Slice_Caster_E</Path>
            </Paths>
            <SortingDepth>Before</SortingDepth>
            <Target>HitTiles</Target>
         </VisualEffect>
         <SoundEffect>
            <Path>ShortSword/Slice/SFX_Skill_Sword_Slice</Path>
            <Delay>0</Delay>
         </SoundEffect>
         <FollowFx>
            <Speed>1000</Speed>
            <Delay>0.13</Delay>
         </FollowFx>

         <CamShake>
            <Id>Hard Hit-CamShake</Id>
            <Delay>0.13</Delay>
         </CamShake>

         <CasterAnim>
            <Delay>0.05</Delay>
         </CasterAnim>
      </CastFXs>
   </SkillDefinition>


