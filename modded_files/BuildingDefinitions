            <GroundCategory>Outside</GroundCategory>

         </GroundCategories>

         <OccupationVolumeType>Ignore</OccupationVolumeType>

         <ConstructionAnimationTypes>

            <ConstructionAnimationType Value="Animated"/>

            <ConstructionAnimationFrameRate Value="20"/>

         </ConstructionAnimationTypes>

      </Construction>



      <Damageable>

         <HealthTotal>50</HealthTotal>

         <TotalPanicValue>0.5</TotalPanicValue>

         <DamagedParticles Id="Damaged_Particles_Enemy"/>

         <!-- <NoSmokeFXOnDestruction/> -->

      </Damageable>



      <Production>

         <BuildingActionDefinitions>

            <BuildingActionDefinition Id="BoneScavengeGold"/>

            <BuildingActionDefinition Id="BoneScavengeMaterials"/>

         </BuildingActionDefinitions>

      </Production>



      <Passives>

         <BuildingPassiveDefinition Id="BonePile1Decay"/>

         <BuildingPassiveDefinition Id="BonePileLevelUpdate"/>

      </Passives>

      

      <Battle>

         <Behavior>

            <ThinkingScope>100</ThinkingScope>

            <Goals>

               <!-- healing bone piles, it's first so we can see the skill when testing the skill -->

               <Goal Id="01_HealingBonePileSkill">

                  <Preconditions>

                     <ConditionsGroup>

                        <ApocalypseSkillProgressionFlagIsToggledCondition Flag="HealingBonePiles"/>

                     </ConditionsGroup>

                     <ConditionsGroup>

                        <NotInFogCondition/>

                     </ConditionsGroup>

                     <ConditionsGroup>

                        <DamageableAroundCondition DamageableType="Enemy" MinAmount="1" MinRange="1" MaxRange="2"/>

                     </ConditionsGroup>

                  </Preconditions>

                  <SkillId Value="BonePileHealing"/>

                  <TargetTypes>

                     <Itself/>

                  </TargetTypes>

                  <TargetingMethod>

                     <FirstTarget/>

                  </TargetingMethod>

               </Goal>

               

               <Goal Id="00_BonePileAttackOnDeath" GoalComputingStep="OnDeath">

                  <Preconditions>

                     <ConditionsGroup>

                        <SkillProgressionFlagIsToggledCondition Flag="ExplosiveBonePiles"/>

                     </ConditionsGroup>

                  </Preconditions>

                  <SkillId Value="BonePileAttackOnDeath"/>

                  <TargetTypes>

                     <Itself/>

                  </TargetTypes>

                  <TargetingMethod>

                     <FirstTarget/>

                  </TargetingMethod>

               </Goal>

            </Goals>

         </Behavior>

      </Battle>

   </BuildingDefinition>



   

