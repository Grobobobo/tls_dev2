            <Speed>10000</Speed>
            <Delay>0.69</Delay>
         </ManeuverFx>

         <CasterAnim>
            <Path>Hero_CastSkill_Teleportation_</Path>
            <Delay>0.69</Delay>
         </CasterAnim>

         <CamShake>
            <Id>Soft Hit-CamShake</Id>
            <Delay>0.69</Delay>
         </CamShake>

         <CamShake>
            <Id>Soft Hit-CamShake</Id>
            <Delay>2</Delay>
         </CamShake>

      </CastFXs>
   </SkillDefinition>


