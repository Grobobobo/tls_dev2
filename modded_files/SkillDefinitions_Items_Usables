   <SkillDefinition Id="HealthPotion">
      <ValidTargets>
         <PlayableUnits/>
      </ValidTargets>
      <UsesPerTurnCount>2</UsesPerTurnCount>
      <Range Min="0" Max="3" Modifiable="true"/>
      <SkillAction>
         <Generic>
            <SkillEffects>
               <RegenStat>
                  <Stat Id="Health"/>
                  <Bonus>20</Bonus>
               </RegenStat>
               <RemoveStatus>
                  <BaseChance>1</BaseChance>
                  <Status>AllNegative</Status>
               </RemoveStatus>
            </SkillEffects>
         </Generic>
      </SkillAction>

      <CastFXs TotalDuration="0.5">
         <VisualEffect>
            <Paths>
               <Path>00_Others/Heal_CasterBefore</Path>
            </Paths>
            <SortingDepth>Before</SortingDepth>
            <Target>HitTiles</Target>
         </VisualEffect>

         <SoundEffect>
            <Path>00_Others/SFX_Skill_Bonus_02</Path>
            <Delay>0</Delay>
         </SoundEffect>

         <CamShake>
            <Id>Soft Hit-CamShake</Id>
            <Delay>0.4</Delay>
         </CamShake>

         <CasterAnim>
            <Path>Hero_CastSkill_Others_HelpBuilding_</Path>
            <Delay>0</Delay>
         </CasterAnim>
      </CastFXs>
   </SkillDefinition>
   

