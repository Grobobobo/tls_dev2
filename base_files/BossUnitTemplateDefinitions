   <BossUnitTemplateDefinition Id="HarpyBoss">

	  <SortingOrderOverride>100</SortingOrderOverride>

      <VisualVariants>

         <VisualVariant Id="01"/>

      </VisualVariants>

      <DamagedParticles Id="Damaged_Particles_Harpy"/>

      <DeathSoundFolderName Value="HarpyBoss"/>

      <Tier>10</Tier>

      <Weight>1</Weight>

      <MoveMethod Method="Flying"/>

      <HealthTotal>1680</HealthTotal>

      <HealthGaugeSize>Large</HealthGaugeSize>

      <MovePointsTotal>5</MovePointsTotal>

      <Accuracy>20</Accuracy>

      <ArmorTotal>120</ArmorTotal>

      <Block>20</Block>

      <Dodge>12</Dodge>

      <Resistance>18</Resistance>

      <StunResistance>1000</StunResistance>

      <DamageSkillId>HarpyFury</DamageSkillId>

      <SkillsToDisplay>

         <SkillToDisplay>HarpyFury</SkillToDisplay>

         <SkillToDisplay>HarpyFlyingAssault</SkillToDisplay>

         <SkillToDisplay>HarpyEnragingScreamAssault</SkillToDisplay>

      </SkillsToDisplay>

      <MoveSpeed>10</MoveSpeed>

      <ExperienceGain>300</ExperienceGain>

      <DamnedSoulsEarned>50</DamnedSoulsEarned>

      <Panic Value="7"/>

      <Behavior>

         <TurnsToSkipOnSpawn Value="1"/>

         <PathfindingStyle>Bresenham</PathfindingStyle>

         <ThinkingScope>1000</ThinkingScope>

         <GoalsComputingOrder Value="-100"/>

         <Goals>

            <!-- Attack Heroes-->

            <Goal Id="01_HarpyFury">

               <SkillId Value="HarpyFury"/>

               <TargetTypes>

                  <PlayableUnit/>

               </TargetTypes>

               <TargetingMethod>

                  <Closest/>

               </TargetingMethod>

            </Goal>

            <!-- Attack Heroes: aggro within 25 tiles-->

            <Goal Id="02_HarpyFlyingAssault">

               <SkillId Value="HarpyFlyingAssault"/>

               <Cooldown Value="3"/>

               <TargetTypes>

                  <PlayableUnit/>

               </TargetTypes>

               <TargetingMethod>

                  <Closest/>

               </TargetingMethod>

            </Goal>

            <!--Attack MagicCircle: default goal-->

            <Goal Id="03_HarpyFury">

               <SkillId Value="HarpyFury"/>

               <TargetTypes>

                  <Building>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetIdCondition>

                              <TargetId Value="MagicCircle"/>

                           </TargetIdCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </Building>

               </TargetTypes>

               <TargetingMethod>

                  <FirstTarget/>

               </TargetingMethod>

            </Goal>

            <!--Buff Enemies-->

            <Goal Id="04_HarpyEnragingScreamAssault">

               <SkillId Value="HarpyEnragingScreamAssault"/>

               <TargetTypes>

                  <EnemyUnit>

                     <TargetConditions>

                        <ConditionsGroup>

                           <DamageableCountInAoeCondition Min="1">

                              <DamageableTypeToCount>Enemy</DamageableTypeToCount>

                           </DamageableCountInAoeCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </EnemyUnit>

               </TargetTypes>

               <TargetingMethod>

                  <FirstTarget/>

               </TargetingMethod>

            </Goal>

            <!-- Attack Obstacles-->

            <Goal Id="05_HarpyFury">

               <SkillId Value="HarpyFury"/>

               <TargetTypes>

                  <Building>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetIdCondition Exclude="true">

                              <TargetsListId Value="EnemiesIgnoredObstacles"/>

                              <TargetId Value="MagicCircle"/>

                           </TargetIdCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </Building>

               </TargetTypes>

               <TargetingMethod>

                  <Closest/>

               </TargetingMethod>

            </Goal>

            

            <Goal Id="06_HarpyAttackOnSpawn" GoalComputingStep="OnSpawn">

