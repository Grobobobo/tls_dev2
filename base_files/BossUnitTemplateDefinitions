            <Goal Id="03_LightFogSpawn">

               <SkillId Value="ZeppelinMist"/>

               <Cooldown Value="2"/>

               <Preconditions>

                  <ConditionsGroup>

                     <InterpretedTurnCondition>SpawnedHour + 1</InterpretedTurnCondition>

                  </ConditionsGroup>

               </Preconditions>

               <TargetTypes>

                  <Itself>

                     <TargetConditions>

                        <ConditionsGroup>

                           <GroundCategoryCondition Id="City"/>

                           <GroundCategoryCondition Id="Outside"/>

                        </ConditionsGroup>

                        <ConditionsGroup>

                           <TargetHasBuildingIdCondition Exclude="true">

                              <BuildingsListId Value="LightFogSpawnersList"/>

                           </TargetHasBuildingIdCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </Itself>

