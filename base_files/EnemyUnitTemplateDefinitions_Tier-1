   <EnemyUnitTemplateDefinition Id="Harpling">

      <VisualVariants>

         <VisualVariant Id="01"/>

      </VisualVariants>

      <DeathSoundFolderName Value="Harpling"/>

      <MoveSoundFolderName Value="Harpling"/>

      <Tier>-1</Tier>

      <Weight>10</Weight>

      <MoveMethod Method="Flying"/>

      <HealthTotal>240</HealthTotal>

      <HealthGaugeSize>Large</HealthGaugeSize>

      <ArmorTotal>0</ArmorTotal>

      <MovePointsTotal>6</MovePointsTotal>

      <Accuracy>10</Accuracy>

      <Block>0</Block>

      <Dodge>20</Dodge>

      <Resistance>12</Resistance>

      <StunResistance>15</StunResistance>

      <DamageSkillId>HarplingClaw</DamageSkillId>

      <SkillsToDisplay>

         <SkillToDisplay>HarplingClaw</SkillToDisplay>

         <SkillToDisplay>HarplingScreech</SkillToDisplay>

      </SkillsToDisplay>

      <MoveSpeed>6</MoveSpeed>

      <ExperienceGain>0</ExperienceGain>

      <DamnedSoulsEarned>10</DamnedSoulsEarned>

      <Panic Value="4"/>

      

      <Behavior>

         <TurnsToSkipOnSpawn Value="1"/>

         <PathfindingStyle>Bresenham</PathfindingStyle>

         <ThinkingScope>100</ThinkingScope>

         <GoalsComputingOrder Value="-50"/>

         <Goals>

            <Goal Id="01_HarplingClaw">

               <SkillId Value="HarplingClaw"/>

               <TargetTypes>

                  <PlayableUnit>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetInRangeCondition Max="Clamp(MovePoints+20,5,30)"/>

                        </ConditionsGroup>

                     </TargetConditions>

                  </PlayableUnit>

               </TargetTypes>

               <TargetingMethod>

                  <Closest/>

               </TargetingMethod>

            </Goal>

            <Goal Id="02_HarplingScreech">

               <SkillId Value="HarplingScreech"/>

               <TargetTypes>

                  <PlayableUnit>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetInRangeCondition Max="Clamp(MovePoints+20,10,30)"/>

                        </ConditionsGroup>

                     </TargetConditions>

                  </PlayableUnit>

               </TargetTypes>

               <TargetingMethod>

                  <Random/>

               </TargetingMethod>

            </Goal>

            <!--Attack MagicCircle: default behaviour to move towards the center-->

            <Goal Id="03_HarplingClaw">

               <SkillId Value="HarplingClaw"/>

               <TargetTypes>

                  <Building>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetIdCondition>

                              <TargetsListId Value="SacredBallistas"/>

                              <TargetId Value="MagicCircle"/>

                           </TargetIdCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </Building>

               </TargetTypes>

               <TargetingMethod>

                  <!--<FirstTarget/>-->

                  <Closest/>

               </TargetingMethod>

            </Goal>

            <Goal Id="04_HarplingClaw">

               <!--Default attack : should never be picked as "intention", but rather as default if the enemy "ends up" next to something to attack with nothing better to do -->

               <SkillId Value="HarplingClaw"/>

               <TargetTypes>

                  <Building>

                     <TargetConditions>

                        <ConditionsGroup>

                           <TargetIdCondition Exclude="true">

                              <TargetsListId Value="EnemiesIgnoredObstacles"/>

                           </TargetIdCondition>

                        </ConditionsGroup>

                     </TargetConditions>

                  </Building>

               </TargetTypes>

               <TargetingMethod>

                  <Closest/>

               </TargetingMethod>

            </Goal>

         </Goals>

      </Behavior>

      

      <Injuries>

         <Injury>

            <BaseRatio>0.7</BaseRatio>

            <RatioMultiplier>0.42</RatioMultiplier>

            <StatModifiers>

               <StatModifier Id="MovePointsTotal" Multiplier="ThreeQuarter"/>

            </StatModifiers>

         </Injury>

         <Injury>

            <BaseRatio>0.3</BaseRatio>

            <RatioMultiplier>0.12</RatioMultiplier>

            <StatModifiers>

               <StatModifier Id="MovePointsTotal" Multiplier="Half"/>

            </StatModifiers>

            <PreventSkills>

               <SkillId>HarplingScreech</SkillId>

            </PreventSkills>

         </Injury>

      </Injuries>

   </EnemyUnitTemplateDefinition>



   

   <EnemyUnitTemplateDefinition Id="NessieOffspring">

