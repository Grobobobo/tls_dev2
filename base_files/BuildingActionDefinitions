<!--
  - BuildingActionDefinition
    - Attribute Id (required) : Id of the BuildingActionDefinition (unique name) => Must be the same in BuildingDefinition.xsd
    - Element PhaseStates (optional) :
      If not set the action will be available during all phases, otherwize you can choose between Available, Disable and Hidden for each phase
         - Element Production (required)
         - Element Deployment (required)
         - Element Night (required)
    - Element WorkersCost (optional) : The workers cost for this action, if not set it will be free
    - Element UsesPerTurnCount (optional) : How many times the action can be use during a turn, if not set there is no limitation
    - Element ActionEffects (required) : The BuildingActionDefinition can ave several effects, current avaible effets are FillGauge, Heal and HealMana
      - Element FillGauge 
         - Element Amount (required) : Must be a int, how many production units are added 
      - Element Heal 
         - Element Amount (required) : Must be a int, hom many hp it can heal
      - Element HealMana 
         - Element Amount (required) : Must be a int, hom many mp it can heal
-->

<BuildingActionDefinitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="BuildingActionDefinitions.xsd">

   <BuildingActionDefinition Id="HelpBlacksmith">
      <PhaseStates>
         <Production>Available</Production>
         <Deployment>Disable</Deployment>
         <Night>Disable</Night>
      </PhaseStates>
      <WorkersCost>2</WorkersCost>
      <UsesPerTurnCount>1</UsesPerTurnCount>
      <ActionEffects>
         <FillGauge>
            <Amount>10</Amount>
         </FillGauge>
      </ActionEffects>
      <CastFXs TotalDuration="0.5">
         <VisualEffect>
            <Paths>
               <Path>00_Others/HelpBuilding_TargetBefore</Path>
            </Paths>
            <SortingDepth>Before</SortingDepth>
            <Target>HitTiles</Target>
         </VisualEffect>

         <SoundEffect>
            <Path>00_Others/Building/SFX_Skill_Building_HelpProduction</Path>
            <Delay>0</Delay>
         </SoundEffect>

         <CamShake>
            <Id>Soft Hit-CamShake</Id>
            <Delay>0.4</Delay>
         </CamShake>
      </CastFXs>
   </BuildingActionDefinition>

