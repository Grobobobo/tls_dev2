   <EnemyAffixDefinition Id="ZeppelinAffix">

      <TokenVariables>

         <TokenVariable Key="StepPercentage" Value="0.3333"/>

         <TokenVariable Key="RemoveStatusType" Value="AllNegative"/>

      </TokenVariables>

      <LocArguments>

         <LocArgument Suffix="%" Style="KeywordNb" Value="Round(StepPercentage * 100)" Interpreted="true"/>



         <StatArgument Stat="HealthTotal"/>

         <LocalizedArgument Value="SkillEffectName_Purge" Style="Purge"/>

         <StatusArgument Status="RemoveStatusType"/>

         <StatusArgument Status="Stun" Chance="100" TurnsCount="1"/>

         <StatusArgument Status="Invulnerable" TurnsCount="1"/>

      </LocArguments>

      <OverrideBoxAsset BoxId="Boss"/>

      <AffixEffect>

         <HealthChunks>

            <Step>StepPercentage</Step>

            <ApplyStatus Status="Stun" TurnsCount="1"/>

            <ApplyStatus Status="Invulnerable" TurnsCount="1"/>

            <RemoveStatus Status="AllNegative"/>

         </HealthChunks>

      </AffixEffect>

   </EnemyAffixDefinition>



