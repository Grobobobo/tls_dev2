   <EnemyAffixDefinition Id="Regenerative">

      <IsEliteAffix Weight="10" LockedByApocalypseFlag="SuperEliteAffixes"/>

      <TokenVariables>

         <TokenVariable Key="HealthPercentage" Value="20"/>

      </TokenVariables>

      <LocArguments>

         <StatArgument Stat="HealthTotal"/>

         <LocArgument Value="HealthPercentage" Style="GoodNb"/>

      </LocArguments>

      <AffixEffect>

         <Regenerative>

            <Percentage>HealthPercentage</Percentage>

         </Regenerative>

      </AffixEffect>

   </EnemyAffixDefinition>

   

   

